            texlength_s="2" 
            texlength_t="1" />

        <material id="ringPost" 
            color="0.8 0.8 0.8 1.0" 
            emissive="0.0 0.0 0.0 0.0" 
			specular="0 0 0 0" 
            shininess="10" 
            textureref="ringPost" 
            texlength_s="1" 
            texlength_t="1" />

        <material id="punchingBagBlack" 
            color="0.8 0.8 0.8 1.0" 
