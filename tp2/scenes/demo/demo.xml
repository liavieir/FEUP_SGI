                <scale value3="1 1 -1" />
            </transforms>
            <children>
                <noderef id="cupFront" />
            </children>
        </node>


        <node id="cupFront">
            <transforms>
                <scale value3="3 3 3" />
            </transforms> 
            <children>
                <primitive>
                    <nurbs degree_u="3" degree_v="3" parts_u="30" parts_v="20" distance="0.0">
