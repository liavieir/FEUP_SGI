                <noderef id="tv" />
                <noderef id="seatsLeft" />
                <noderef id="seatsRight" />
                <noderef id="seatsBack" />
                <noderef id="seatsFront" />
                <noderef id="sky" />
                <noderef id="spotlights" />
                <noderef id="chair1" />
                <noderef id="chair2" />
                <noderef id="carpet" />
                <noderef id="cup" />
                
				 

                <directionallight id="directional"
                    color="1 1 1 1"
                    intensity="0.3"
                    position="0 50 0"
                    castshadow="false"
                />

                <directionallight id="directional2"
                    color="1 1 1 1"
