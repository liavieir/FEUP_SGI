                        <controlpoint xx="0.125" yy="0.5" zz="0.0" />
                        <controlpoint xx="0.25" yy="1.0" zz="0.0" />
                            
                        </nurbs>
                </primitive>
            </children>
        </node>


        <node id="rectangleBack"> 
            <materialref id="ringFloorSides" />
            <transforms>
				<translate value3="0 0 -5" />
				<rotate value3="0 180.0 0.0" />
