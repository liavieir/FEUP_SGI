            near="0.1" 
            far="1000" 
            location="40 50 50" 
            target="0 0.0 0" /> 

        <perspective id="cameraView" 
            angle="60" 
            near="0.1" 
            far="1000" 
            location="-10 11 23" 
            target="0 0 0" />
        
        <perspective id="perspective2" 
