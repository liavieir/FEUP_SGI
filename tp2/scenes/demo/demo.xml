                        <controlpoint xx="-1.0" yy="0.0" zz="0" />
                        <controlpoint xx="-0.125" yy="0.5" zz="0" />
                        <controlpoint xx="-0.25" yy="1.0" zz="0" />

                        
                        <controlpoint xx="-0.45" yy="-1.0" zz="0.5" />
                        <controlpoint xx="-0.45" yy="-0.5" zz="1.0" />
                        <controlpoint xx="-0.45" yy="-0.5" zz="0" />
                        <controlpoint xx="-0.25" yy="1.0" zz="0.35" />

                        
                        <controlpoint xx="0.45" yy="-1.0" zz="0.5" />
