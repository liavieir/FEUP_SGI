            textureref="lightWood"
            texlength_s="1"
            texlength_t="1" />

        <material id="seats"
            color="0.8 0.8 0.8 1.0" 
            emissive="0.0 0.0 0.0 0.0"
            specular="0 0 0 0"
            shininess="10"
            textureref="seats"
            texlength_s="1"
            texlength_t="1" />

        <material id="steel"
            color="0.7 0.7 0.7 1.0"
            emissive="0.1 0.1 0.1 1.0"
            specular="0.2 0.2 0.2 1.0"
            shininess="10"
            textureref="steel"
            texlength_s="1"
            texlength_t="1" 
        />

        <material id="vase" 
            color="0.8 0.8 0.8 1.0" 
            emissive="0.0 0.0 0.0 0.0" 
			specular="0 0 0 0" 
            shininess="10" 
            textureref="vase" 
            texlength_s="1" 
            texlength_t="1" 
            twosided = "true" />

        <material id="spotlight"
            color="0.0 0.0 0.0 1"
            emissive="0.0 0.0 0.0 1.0"
            specular="0 0 0 0"
            shininess="0"
            shading="none"/>
                
        <material id="rubberBump"
            color="1.0 1.0 1.0 1.0" 
            emissive="0.0 0.0 0.0 1.0"
            specular="0 0 0 0"
            shininess="0"
            textureref="rubberColor"
            bumpref="rubberHeight"
            specularref="rubberSpec"
            bumpscale="0.9"
            texlength_s="1"
            texlength_t="1" 
            twosided="true"/>
        
        <material id="video1"
            color="1.0 1.0 1.0 1.0" 
            emissive="0.0 0.0 0.0 1.0"
            specular="0 0 0 0"
            shininess="10"
            textureref="video"
            texlength_s="1"
            texlength_t="1" 
            />

			
